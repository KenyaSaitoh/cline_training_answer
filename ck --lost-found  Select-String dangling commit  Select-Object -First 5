[31m-    echo "Aborting $PROGNAME:  $*" 1>&2[m
[31m-    exit 1[m
[31m-}[m
[31m-[ -n "$TMPDIR" ] || TMPDIR=/tmp[m
[31m-WORKDIR="$TMPDIR/${PROGNAME%.*}-$$"[m
[31m-mkdir "$WORKDIR" || Failout "Failed to make work director '$WORKDIR'"[m
[31m-[m
